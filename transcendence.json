{
  "$schema": "https://lnav.org/schemas/format-v1.schema.json",
  "transcendence": {
    "description": "log format for ft_transcendence",
    "regex": {
      "std": {
        "pattern": "^(?<timestamp>.+):\\s(?<package>frontend|backend).*?:\\s(?<body>.*)$"
      }
    },
    "value": {
      "package": {
        "kind": "string",
        "identifier": true
      },
      "timestamp": {
        "kind": "string"
      },
      "body": {
        "identifier": false,
        "description": "body"
      }
    },
    "module-field": "package",
    "sample": [
      {
        "line": "Fri Aug 29 23:13:07 JST 2025: backend dev: CLI Building entry: src/server.ts"
      },
      {
        "line": "Fri Aug 29 23:13:07 JST 2025: frontend dev:   VITE v7.1.3  ready in 232 ms"
      }
    ]
  }
}
